msgstr "Einstellungen"

#: wp-mail-validator.php:125
msgid "Default-gateway IP"
msgstr "Standard-Gateway IP"

#: wp-mail-validator.php:127
msgid "Leave blank to use the default gateway"
msgstr "Leer, um das Default-Gateway zu nutzen"

#: wp-mail-validator.php:134
msgid "Validate Registrations"
msgstr "Registrierungen validieren"

#: wp-mail-validator.php:140 wp-mail-validator.php:155
#: wp-mail-validator.php:170 wp-mail-validator.php:185
#: wp-mail-validator.php:200 wp-mail-validator.php:215
